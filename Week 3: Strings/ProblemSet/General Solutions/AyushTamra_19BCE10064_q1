#include<bits/stdc++.h>
using namespace std;

int main(int argc, char const *argv[])
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string str, word;
    getline(cin, str);
    stringstream stream(str);
    stream >> word;
    reverse(str.begin(),str.begin()+word.length());
    cout<<str;
    return 0;
}
